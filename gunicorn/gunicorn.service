[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/d/FinanceTracker/mysite
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/d/FinanceTracker/mysite/mysite/blog.sock blog.wsgi:application

       
[Install]
WantedBy=multi-user.target